{
    "name": "klausi/github_drupalorg",
    "type": "application",
    "description": "Synchronization of github pull requests to drupal.org",
    "keywords": ["scraper"],
    "license": "GPLv2+",
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/dawehner/DomCrawler.git"
        }
    ],
    "require": {
        "fabpot/goutte": "*",
        "guzzle/plugin-cookie": "*",
        "symfony/event-dispatcher": "*",
        "symfony/dom-crawler": "dev-master#ed6bf2dad3a56a5c83ef827e580250d42ac740af"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\github_drupalorg\\": "lib/"
        }
    }
}
